import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    double length;
    double height;
    int size;
    int estimate;
    Scanner scan = new Scanner(System.in);
    System.out.println("Enter jelly bean length (cm): ");
    length = scan.nextDouble();
    System.out.println("Enter jelly bean height (cm): ");
    height = scan.nextDouble();
    System.out.println("Enter jar size (mL): ");
    size = scan.nextInt();
    estimate = (int)((size * 0.698)/(5 * 3.14 * length * (height*height / 24)));
    System.out.println("Estimate for number of jelly beans with average length :" + length + "\n" + "height: " + height + "\n" + "in a jar of size: " + size + "\n" + "the estimate is " + estimate);
  }
}
